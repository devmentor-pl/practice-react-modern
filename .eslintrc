{
    "parser": "@babel/eslint-parser",
    "extends": ["airbnb", "prettier"],
    "rules": {
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [".js"]
            }
        ],
        "indent": ["error", 4],
        "react/jsx-indent": ["error", 4],
        "react/jsx-indent-props": ["error", 4],
        "func-names": ["error", "as-needed"],
        "max-len": ["error", { "code": 100 }],
        "quotes": ["error", "single"]
    },
    "globals": {
        "window": true,
        "document": true
    }
}
