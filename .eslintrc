{
  "parser": "@babel/eslint-parser",
  "extends": [
    "airbnb",
    "airbnb/hooks",
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js"
        ]
      }
    ],
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1
      }
    ],
    "react/jsx-indent": [
      "error",
      4
    ],
    "react/jsx-indent-props": [
      "error",
      4
    ],
    "react/state-in-constructor": [
      0,
      "never"
    ],
    "import/no-named-as-default": 0
  },
  "globals": {
    "window": true,
    "document": true
  }
}
