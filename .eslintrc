{
    "parser": "@babel/eslint-parser",
    "extends" : ["airbnb", "prettier"],
    "rules": {
        "react/jsx-filename-extension": [1, {
            "extensions": [".js", ".jsx"] 
        }],
        "indent": ["error", 4],
        "react/jsx-indent": ["error", 4],
        "react/jsx-indent-props": ["error", 4],
        "react/state-in-constructor": [0, "never"]
    },
    "globals": {
        "window": true,
        "document": true
    }
}